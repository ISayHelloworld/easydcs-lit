<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.control.Label?>
<VBox styleClass="VBstyle" style="-fx-spacing:20" fx:id="root" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
    <VBox styleClass="card">
        <Label styleClass="phaseTitle" text="%dataExport"/>
        <HBox styleClass="HBstyle" alignment="CENTER_LEFT">
            <Label text="%dataExport"/>
            <ComboBox fx:id="tableList" promptText="%unChoose"/>
            <Button styleClass="btnNormal"  fx:id="query" text="%query">
                <HBox.margin>
                    <Insets left="10.0"/>
                </HBox.margin>
            </Button>
        </HBox>
    </VBox>
    <TableView fx:id="exportTable"/>
    <Button styleClass="btnNormal" style="-fx-font-size:14" fx:id="export" text="%export"/>
</VBox>
